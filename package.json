{
  "name": "pump-probe",
  "version": "1.0.0",
  "description": "Pumping Data & Probe Behavior",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest -c ./jest.config.json",
    "build": "tsc -p ./tsconfig.json",
    "build:watch": "npm run clean && npm run build -- --watch",
    "prettier": "prettier --config .prettierrc --write src/*.{ts,tsx}",
    "clean": "rimraf ./lib",
    "version_up:major": "changelog -M && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version major",
    "version_up:minor": "changelog -m && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version minor",
    "version_up:patch": "changelog -p && git add CHANGELOG.md && git commit -m \"updated CHANGELOG.md\" && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Himenon/PumpProbeJs.git"
  },
  "files": [
    "lib",
    "package.json"
  ],
  "keywords": [
    "typescript",
    "debug"
  ],
  "author": "K.Himeno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Himenon/PumpProbeJs/issues"
  },
  "homepage": "https://github.com/Himenon/PumpProbeJs#readme",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "generate-changelog": "^1.7.1",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^7.1.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "express": "^4.16.3",
    "hyperapp": "^1.2.8"
  }
}
